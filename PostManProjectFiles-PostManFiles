							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Response time is less than 200ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify response data is valid\", function () {\r",
									"    const response = pm.response.json();\r",
									"\r",
									"    // Validation rules\r",
									"    const rules = {\r",
									"        firstName: value => /^[A-Za-z]+$/.test(value), // String with no numbers\r",
									"        lastName: value => /^[A-Za-z]+$/.test(value), // String with no numbers\r",
									"        email: value => /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/.test(value), // Valid email format\r",
									"        password: value => value.length >= 8, // At least 8 characters\r",
									"        birthdate: value => /^\\d{1,2}-\\d{1,2}-\\d{4}$/.test(value), // Valid date format (DD-MM-YYYY)\r",
									"        phone: value => /^\\d{10}$/.test(value), // 10 digits\r",
									"        street1: value => typeof value === \"string\" && value.trim() !== \"\", // Non-empty string\r",
									"        street2: value => typeof value === \"string\" && value.trim() !== \"\", // Non-empty string\r",
									"        city: value => /^[A-Za-z\\s]+$/.test(value), // Letters and spaces only\r",
									"        postalCode: value => /^[A-Z0-9\\-]+$/.test(value), // Alphanumeric with hyphens\r",
									"        country: value => /^[A-Za-z\\s]+$/.test(value) // Letters and spaces only\r",
									"    };\r",
									"\r",
									"    // Loop through rules and validate dynamically\r",
									"    for (const key in rules) {\r",
									"        if (response[key] !== undefined) {\r",
									"            const isValid = rules[key](response[key]);\r",
									"            pm.expect(isValid, `${key} is invalid`).to.be.true;\r",
									"        } else {\r",
									"            pm.expect.fail(`${key} is missing in the response`);\r",
									"        }\r",
									"    }\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
